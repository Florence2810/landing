/* Your card CSS file */
.card-category{
	width: 200px;
	height: 200px;
	color: white;
	display:inline-flex;
	justify-content: center;
	align-items: items;
	background-color: #E72676;
	background-size: cover;
	border-radius: 10px;
	box-shadow: 0 0 10px rgba(0,0,0,1);

}

.card-product{
	display: flex;
	background-color: white;
	box-shadow: 0 0 10px rgba(0,0,0,1);
}
.card-product > img {
	width: 100px;
	height: 100px;
	object-fit: cover;
}

.card-trip{
	width: 40px;
	background-color: white;
	box-shadow: 0 0 10px rgba(0,0,0,0.2);
}
.card-trip > img {
	height: 200px;
	width: 400px;
	object-fit: cover;
}
